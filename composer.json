{
  "name": "hylianshield/validator",
  "type": "library",
  "description": "Data validation",
  "homepage": "https://github.com/HylianShield/validator",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Remko Silvis",
      "role": "developer"
    },
    {
      "name": "Jan-Marten de Boer",
      "role": "developer"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.5.3",
    "ext-json": "^1.4.0",
    "ext-SPL": "^7.0.0"
  },
  "autoload": {
    "psr-4": {
      "HylianShield\\Validator\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "HylianShield\\Tests\\Validator\\": "tests"
    }
  },
  "suggest": {
    "hylianshield/validator-pcre": "Use PCRE regular expressions as validator."
  },
  "archive": {
    "exclude": [
      "tests",
      "examples",
      ".gitignore",
      ".travis.yml",
      "phpunit.xml"
    ]
  }
}
